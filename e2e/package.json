{
	"name": "carpmanager-cyp",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"vite": "vite -- --port 5273",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"deploy": "tsc -b && vite build && firebase deploy --only hosting",
		"cypress": "cypress open",
		"cypress:headless": "cypress run",
		"cypress:crud": "npx cypress run --spec \"cypress/e2e/**/*crud*.cy.{js,jsx,ts,tsx}\" --browser edge --headed"
	},
	"dependencies": {
		"axios": "^1.12.2",
		"imap-simple": "^5.1.0",
		"mailparser": "^3.7.5",
		"nodemailer": "^7.0.9",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.9.4"
	},
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"@faker-js/faker": "^10.1.0",
		"@types/imap-simple": "^4.2.10",
		"@types/mailparser": "^3.4.6",
		"@types/node": "^24.6.0",
		"@types/nodemailer": "^7.0.2",
		"@types/react": "^19.1.16",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react-swc": "^4.1.0",
		"cypress": "^13.17.0",
		"eslint": "^9.36.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.22",
		"globals": "^16.4.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.45.0",
		"vite": "^7.1.7"
	}
}
